<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

	<application>
		<locale-config>
			<default-locale>en</default-locale>
			<supported-locale>de</supported-locale>
		</locale-config>
		<message-bundle>de.beosign.quizzer.resources.messages</message-bundle>
		<resource-bundle>
			<base-name>de.beosign.quizzer.resources.messages</base-name>
			<var>msg</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>de.beosign.quizzer.resources.enums</base-name>
			<var>enums</var>
		</resource-bundle>

	</application>
</faces-config>